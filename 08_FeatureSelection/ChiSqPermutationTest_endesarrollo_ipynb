    "diffs = []\n",
    "for i in range(1000):\n",
    "    random.shuffle(box)\n",
    "    s1 = box[0:female_sample_size]\n",
    "    s2 = box[female_sample_size:]\n",
    "    ts1 = sum(s1)\n",
    "    ts2 = sum(s2)\n",
    "    diffs.append(ts2-ts1)\n",
