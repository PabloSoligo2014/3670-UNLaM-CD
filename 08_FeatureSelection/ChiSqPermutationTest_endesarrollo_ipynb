   "source": [
    "survived_count =df[df[\"Survived\"]==1].shape[0]\n",
    "died_count = df[df[\"Survived\"]==0].shape[0] \n",
    "female_sample_size = df[df[\"Sex\"]==\"female\"].shape[0] \n",
    "male_sample_size   = df[df[\"Sex\"]==\"male\"].shape[0]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "metadata": {},
   "outputs": [],
   "source": [
    "diff = df[(df[\"Survived\"]==1) & (df[\"Sex\"]==\"female\")].shape[0]-df[(df[\"Survived\"]==1) & (df[\"Sex\"]==\"male\")].shape[0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Resampling approach\n",
    "# una caja con la totalidad de sobrevivientes\n",
    "box = [1] * survived_count\n",
    "# agrego a la caja los fallecidos\n",
    "box.extend([0] * died_count)\n",
    "\n"
   ]
  },
  {
